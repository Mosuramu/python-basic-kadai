        "class Human:\n",
        "  def __init__(self):\n",
        "    self.name = \"\"\n",
        "    self.age = \"\"\n",
        "\n",
        "  def printinfo(self, name, age):\n",
        "    self.name = name\n",
        "    self.age = age\n",
        "\n",
        "human = Human()\n",
        "human.printinfo(\"太郎\", 20)\n",
        "print(human.name)\n",
        "print(human.age)"
